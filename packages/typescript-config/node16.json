{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "extends": "./base2.json",
  "compilerOptions": {
    "lib": ["es2022", "DOM", "DOM.Iterable"],
    "target": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
